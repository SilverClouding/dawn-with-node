    <div class="style-guide__subheading">
      <h4 class="heading-4">Text Input</h4>
    </div>
    <div class="style-guide__info">
      <p>Wrapper class: <span class="style-guide__code">.input-wrapper</span></p>
      <p>Label class: <span class="style-guide__code">.label</span></p>
      <p>Input class: <span class="style-guide__code">.text-input</span></p>
    </div>
    <div class="style-guide__flex style-guide__flex--top">
      <div class="input-wrapper">
        <label class="label" for="text-input-value">Empty</label>
        <input id="text-input-value" class="text-input" type="text" value="">
      </div>
      <div class="input-wrapper">
        <label class="label" for="text-input-placeholder">Placeholder</label>
        <input id="text-input-placeholder" class="text-input" type="text" placeholder=".text-input">
      </div>
      <div class="input-wrapper">
        <label class="label" for="text-input-success">Valid</label>
        <input id="text-input-success" class="text-input" type="text" value=".text-input" required>
        <p class="input-feedback input-feedback--success">.input-feedback.input-feedback--success</p>
      </div>
      <div class="input-wrapper">
        <label class="label" for="text-input-error">Invalid</label>
        <input id="text-input-error" class="text-input" type="text" value=".text-input" required pattern="[a-zA-Z0-9]">
        <p class="input-feedback input-feedback--error">.input-feedback.input-feedback--error</p>
      </div>
    </div>

    <div class="style-guide__subheading">
      <h4 class="heading-4">Select</h4>
    </div>
    <div class="style-guide__info">
