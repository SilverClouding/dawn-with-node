        <label class="checkbox-label" for="checkbox_input"> Confirm the checkbox </label>
      </div>
      <div class="checkbox-wrapper">
        <input type="checkbox" id="checkbox_input" value="1" class="checkbox" checked>
        <label class="checkbox-label" for="checkbox_input"> Confirm the checkbox </label>
      </div>
    </div>

