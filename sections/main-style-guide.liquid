    <div class="style-guide__flex style-guide__flex--top">
      <div class="quantity-selector">
        <button class="quantity-selector__btn quantity-selector__btn--dec" aria-label="-"></button>
        <input type="number" id="quantity" class="quantity-selector__input" value="1">
        <button class="quantity-selector__btn quantity-selector__btn--inc" aria-label="+"></button>
      </div>
    </div>

