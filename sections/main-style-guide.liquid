        <input type="radio" id="radio_input_1" name="radio_group" value="1" class="radio">
        <label class="radio-label" for="radio_input_1"> Click a radio </label>
      </div>
      <div class="radio-wrapper">
        <input type="radio" id="radio_input_2" name="radio_group" value="1" class="radio" checked>
        <label class="radio-label" for="radio_input_2"> Click a radio </label>
